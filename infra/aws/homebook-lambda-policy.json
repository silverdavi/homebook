{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3WorksheetAccess",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::homebook-worksheets",
        "arn:aws:s3:::homebook-worksheets/*"
      ]
    },
    {
      "Sid": "CloudWatchLogs",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:us-east-1:302249171798:log-group:/aws/lambda/homebook-*:*"
    }
  ]
}
